{
  "name": "react-albus",
  "version": "2.0.0",
  "description": "React component library for building declarative multi-step flows.",
  "files": [
    "lib"
  ],
  "main": "lib",
  "scripts": {
    "clean": "rimraf lib",
    "start": "webpack serve --port 3000 --open",
    "test": "jest",
    "lint": "eslint --ignore-path .eslintignore --ext .js --ext .jsx ./",
    "prebuild": "npm run clean && npm run lint",
    "build": "babel src -d lib --copy-files",
    "posttest": "npm run lint",
    "prepublish": "npm run build"
  },
  "repository": "americanexpress/react-albus",
  "keywords": [
    "react",
    "react-component",
    "wizard",
    "step",
    "multistep",
    "routing"
  ],
  "authors": [
    "Jack Cross <jack.cross1@aexp.com> (https://github.com/jayjaycross)",
    "Nathan Force <nathan.a.force@aexp.com> (https://github.com/nathanforce)"
  ],
  "license": "Apache-2.0",
  "jest": {
    "preset": "amex-jest-preset-react",
    "testPathIgnorePatterns": [
      ".eslintrc.js"
    ]
  },
  "dependencies": {
    "history": "^4.10.1",
    "hoist-non-react-statics": "^3.2.1",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-react": "^7.18.6",
    "amex-jest-preset-react": "^8.0.1",
    "babel-eslint": "^10.1.0",
    "babel-preset-amex": "^3.5.0",
    "coveralls": "^3.1.1",
    "css-loader": "^3.3.1",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.32.0",
    "eslint-config-amex": "^15.0.0",
    "eslint-plugin-jest": "^24.5.0",
    "eslint-plugin-jest-dom": "^3.9.2",
    "import-glob-loader": "^1.1.0",
    "jest": "^27.5.1",
    "node-sass": "^8.0.0",
    "prettier": "^2.8.4",
    "rc-progress": "^2.2.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.8.1",
    "react-test-renderer": "^17.0.2",
    "react-transition-group": "^4.4.1",
    "rimraf": "^4.1.2",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-dev-server": "^4.11.1"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0"
  }
}
