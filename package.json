{
  "name": "react-albus",
  "version": "2.0.0",
  "description": "React component library for building declarative multi-step flows.",
  "files": [
    "lib"
  ],
  "main": "lib",
  "scripts": {
    "clean": "rimraf lib",
    "start": "webpack-dev-server --port 3000 --inline --hot --open",
    "test": "jest",
    "lint": "eslint --ignore-path .eslintignore --ext .js --ext .jsx ./",
    "lint:fix": "eslint --ignore-path .eslintignore --fix --ext .js --ext .jsx ./",
    "prebuild": "npm run clean && npm run lint",
    "build": "babel src -d lib --copy-files",
    "posttest": "npm run lint",
    "prepublish": "npm run build"
  },
  "repository": "americanexpress/react-albus",
  "keywords": [
    "react",
    "react-component",
    "wizard",
    "step",
    "multistep",
    "routing"
  ],
  "authors": [
    "Jack Cross <jack.cross1@aexp.com> (https://github.com/jayjaycross)",
    "Nathan Force <nathan.a.force@aexp.com> (https://github.com/nathanforce)"
  ],
  "license": "Apache-2.0",
  "jest": {
    "preset": "amex-jest-preset-react",
    "testPathIgnorePatterns": [
      ".eslintrc.js"
    ]
  },
  "dependencies": {
    "babel-loader": "^9.1.2",
    "history": "^4.6.0",
    "hoist-non-react-statics": "^2.3.1",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/preset-react": "^7.18.6",
    "amex-jest-preset": "^7.0.0",
    "amex-jest-preset-react": "^7.0.0",
    "babel-preset-amex": "^3.3.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "coveralls": "^2.13.1",
    "css-loader": "^0.26.2",
    "enzyme": "^3.1.0",
    "enzyme-to-json": "^3.1.4",
    "eslint": "^7.2.0",
    "eslint-config-amex": "^15.3.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "import-glob-loader": "^1.1.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.0.2",
    "rc-progress": "^2.2.5",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.0.0",
    "react-test-renderer": "^16.0.0",
    "react-transition-group": "^2.2.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.2",
    "webpack": "^5.77.0",
    "webpack-dev-server": "^4.13.1"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0"
  }
}
