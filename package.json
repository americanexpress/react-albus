{
  "name": "react-albus",
  "version": "2.0.0",
  "description": "React component library for building declarative multi-step flows.",
  "files": [
    "lib"
  ],
  "main": "lib",
  "scripts": {
    "clean": "rimraf lib",
    "start": "webpack-dev-server --port 3000 --inline --hot --open",
    "test": "jest --passWithNoTests",
    "lint": "eslint --ignore-path .eslintignore --ext .js --ext .jsx ./",
    "prebuild": "npm run clean && npm run lint",
    "build": "babel src -d lib --copy-files",
    "posttest": "npm run lint",
    "prepublish": "npm run build"
  },
  "repository": "americanexpress/react-albus",
  "keywords": [
    "react",
    "react-component",
    "wizard",
    "step",
    "multistep",
    "routing"
  ],
  "authors": [
    "Jack Cross <jack.cross1@aexp.com> (https://github.com/jayjaycross)",
    "Nathan Force <nathan.a.force@aexp.com> (https://github.com/nathanforce)"
  ],
  "license": "Apache-2.0",
  "jest": {
    "preset": "amex-jest-preset-react",
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/*"
    ]
  },
  "dependencies": {
    "history": "^4.6.0",
    "hoist-non-react-statics": "^2.3.1",
    "prop-types": "^15.5.8"
  },
  "devDependencies": {
    "amex-jest-preset-react": "^5.0.2",
    "babel-preset-amex": "^3.2.0",
    "coveralls": "^2.13.1",
    "css-loader": "^0.26.2",
    "enzyme": "^3.1.0",
    "enzyme-to-json": "^3.1.4",
    "eslint": "^6.7.2",
    "eslint-config-amex": "^11.1.0",
    "import-glob-loader": "^1.1.0",
    "jest": "^24.9.0",
    "node-sass": "^4.5.0",
    "rc-progress": "^2.2.5",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.0.0",
    "react-test-renderer": "^16.0.0",
    "react-transition-group": "^2.2.1",
    "rimraf": "^3.0.0",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
    "webpack": "^2.0.0",
    "webpack-dev-server": "^2.0.0"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
  }
}
