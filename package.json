{
  "name": "react-albus",
  "version": "2.0.0",
  "description": "React component library for building declarative multi-step flows.",
  "files": [
    "lib"
  ],
  "bugs": {
    "url": "https://github.com/americanexpress/react-albus/issues"
  },
  "main": "lib",
  "scripts": {
    "clean": "rimraf lib",
    "start": "webpack-dev-server --port 3000 --inline --hot --open",
    "test": "npm run test:unit",
    "test:unit": "jest",
    "test:git-history": "commitlint --from origin/master --to HEAD",
    "test:lint": "eslint --ignore-path .eslintignore --ext .js --ext .jsx ./",
    "prebuild": "npm run clean && npm run test:lint",
    "build": "babel src -d lib --copy-files",
    "posttest": "npm run test:lint && npm run test:git-history",
    "prepublish": "npm run build"
  },
  "repository": "americanexpress/react-albus",
  "keywords": [
    "react",
    "react-component",
    "wizard",
    "step",
    "multistep",
    "routing"
  ],
  "authors": [
    "Jack Cross <jack.cross1@aexp.com> (https://github.com/jayjaycross)",
    "Nathan Force <nathan.a.force@aexp.com> (https://github.com/nathanforce)"
  ],
  "license": "Apache-2.0",
  "jest": {
    "preset": "amex-jest-preset-react",
    "testPathIgnorePatterns": [
      ".eslintrc.js"
    ]
  },
  "dependencies": {
    "history": "^4.6.0",
    "hoist-non-react-statics": "^2.3.1",
    "prop-types": "^15.5.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@semantic-release/changelog": "^3.0.5",
    "@semantic-release/commit-analyzer": "^6.3.2",
    "@semantic-release/git": "^7.0.18",
    "@semantic-release/npm": "^5.3.4",
    "@semantic-release/release-notes-generator": "^7.3.2",
    "amex-jest-preset-react": "^4.0.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "coveralls": "^2.13.1",
    "css-loader": "^0.26.2",
    "enzyme": "^3.1.0",
    "enzyme-to-json": "^3.1.4",
    "eslint": "^3.19.0",
    "eslint-config-amex": "^6.0.0",
    "eslint-config-prettier": "^2.2.0",
    "eslint-plugin-prettier": "^2.1.2",
    "husky": "^3.0.9",
    "import-glob-loader": "^1.1.0",
    "jest": "^21.2.1",
    "node-sass": "^4.5.0",
    "prettier": "^1.6.1",
    "rc-progress": "^2.2.5",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.0.0",
    "react-test-renderer": "^16.0.0",
    "react-transition-group": "^2.2.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^6.0.2",
    "semantic-release": "^15.13.31",
    "style-loader": "^0.13.2",
    "webpack": "^2.0.0",
    "webpack-dev-server": "^2.0.0"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/npm"
    ],
    "branch": "master"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
