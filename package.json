{
  "name": "react-albus",
  "version": "2.0.0",
  "description": "React component library for building declarative multi-step flows.",
  "files": [
    "lib"
  ],
  "main": "lib",
  "scripts": {
    "clean": "rimraf lib",
    "start": "webpack-dev-server --port 3000 --inline --hot --open",
    "test": "jest",
    "lint": "eslint --ignore-path .eslintignore --ext .js --ext .jsx ./",
    "lint:fix": "eslint --fix --ignore-path .eslintignore --ext .js --ext .jsx ./",
    "prebuild": "npm run clean && npm run lint",
    "build": "babel src -d lib --copy-files",
    "posttest": "npm run lint",
    "prepublish": "npm run build"
  },
  "repository": "americanexpress/react-albus",
  "keywords": [
    "react",
    "react-component",
    "wizard",
    "step",
    "multistep",
    "routing"
  ],
  "authors": [
    "Jack Cross <jack.cross1@aexp.com> (https://github.com/jayjaycross)",
    "Nathan Force <nathan.a.force@aexp.com> (https://github.com/nathanforce)"
  ],
  "license": "Apache-2.0",
  "jest": {
    "testPathIgnorePatterns": [
      ".eslintrc.js"
    ]
  },
  "dependencies": {
    "history": "^5.3.0",
    "hoist-non-react-statics": "^3.3.2",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^9.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^3.1.1",
    "css-loader": "^6.7.3",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "escope": "^4.0.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "import-glob-loader": "^1.1.0",
    "jest": "^29.5.0",
    "node-sass": "^8.0.0",
    "prettier": "2.8.7",
    "rc-progress": "^3.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "react-test-renderer": "^18.2.0",
    "react-transition-group": "^4.4.5",
    "rimraf": "^4.4.1",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.2",
    "webpack": "^5.76.3",
    "webpack-dev-server": "^4.13.1"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  }
}
